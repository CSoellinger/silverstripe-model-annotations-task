{
    "name": "csoellinger/silverstripe-model-annotation-task",
    "description": "A SilverStripe Task to generate data object model annotations for defined db fields. Also for configs from data extensions.",
    "license": "BSD-3-Clause",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "silverstripe-module",
        "silverstripe-vendormodule",
        "module",
        "vendormodule",
        "plugin",
        "addon",
        "Model",
        "Database",
        "IDE",
        "Annotation",
        "Generator",
        "PHP"
    ],
    "readme": "README.md",
    "authors": [
        {
            "name": "CSoellinger",
            "email": "christopher.soellinger@gmail.com",
            "role": "developer"
        }
    ],
    "homepage": "https://github.com/CSoellinger/silverstripe-model-annotation-task",
    "support": {
        "issues": "https://github.com/CSoellinger/silverstripe-model-annotation-task/issues"
    },
    "require": {
        "php": ">=7.3,<8.1",
        "ext-ast": "*",
        "silverstripe/framework": "~4.10"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^1.4",
        "phpstan/phpstan-phpunit": "^1.1",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16.1",
        "ramsey/devtools": "^1.7",
        "ramsey/devtools-lib": "1.3",
        "silverstripe/sqlite3": "^2",
        "squizlabs/php_codesniffer": "3.*",
        "vimeo/psalm": "^4.22"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "CSoellinger\\SilverStripe\\ModelAnnotation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CSoellinger\\SilverStripe\\ModelAnnotation\\Test\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "composer/installers": true,
            "captainhook/plugin-composer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "phpstan/extension-installer": true,
            "ramsey/composer-repl": true,
            "ramsey/devtools": true,
            "silverstripe/vendor-plugin": true,
            "composer/package-versions-deprecated": true
        },
        "sort-packages": true
    },
    "extra": {
        "captainhook": {
            "force-install": true
        },
        "ramsey/conventional-commits": {
            "configFile": "conventional-commits.json"
        }
    }
}
